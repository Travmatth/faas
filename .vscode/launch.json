{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connect to Container Server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
			"remotePath": "/usr/qaas",
            "port": 2345,
            "host": "localhost",
			"showLog": true,
			"trace": "log",
			"logOutput": "rpc"
        },
        {
            "name": "Launch Local Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/qaas/main.go",
            "env": {
                "QAAS_CONFIG": "${workspaceFolder}/configs/httpd.yml"
            },
            "args": [
                "--env", "DEVELOPMENT",
                "--ip", "127.0.0.1",
                "--port", ":8080",
                "--static", "${workspaceFolder}/web/www/static"
            ]
        }
    ]
}